  "semanticTokenColors": {
    "comment": {
      "foreground": "#908a82",
      "fontStyle": "italic"
    },
    "keyword": "#c48573",
    "string": "#95ad63",
    "function": {
      "foreground": "#e0a46a",
      "fontStyle": "bold"
    },
    "function.declaration": "#e0a46a",
    "method": {
      "foreground": "#e0a46a",
      "fontStyle": "bold"
    },
    "method.declaration": "#e6b277",
    "property": "#c7d4e2",
    "property.readonly": "#e2c48f",
    "type": "#d6b56f",
    "class": {
      "foreground": "#d6b56f",
      "fontStyle": "bold"
    },
    "interface": "#c2cdda",
    "enum": "#d6b56f",
    "enumMember": "#e8b860",
    "typeParameter": {
      "foreground": "#c7d4e2",
      "fontStyle": "italic"
    },
    "parameter": "#c2cdda",
    "parameter.readonly": "#f2d49f",
    "variable": "#e3e0dc",
    "variable.readonly": "#e2c48f",
    "variable.constant": "#a08550",
    "variable.global": "#e3e0dc",
    "variable.defaultLibrary": {
      "foreground": "#c7d4e2",
      "fontStyle": "italic"
    },
    "event": "#c48573",
    "macro": "#c48573",
    "decorator": {
      "foreground": "#d6b56f",
      "fontStyle": "italic"
    },
    "keyword.decorator": {
      "foreground": "#d6b56f",
      "fontStyle": "italic"
    },
    "label": "#a19a91",
    "builtinType": "#d6b56f",
    "operator": "#e3e0dc",
    "string.regexp": "#c7d4e2",
    "formatSpecifier": "#c7d4e2",
    "escape": "#c7d4e2",
    "number": "#e6c491",
    "boolean": "#ce6f65",
    "typeAlias": "#d6b56f",
    "namespace": "#bda980",
    "struct": "#d6b56f",
    "modifier": "#c48573"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#908a82"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#c48573"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#95ad63"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#a08550"
      }
    },
    {
      "name": "Regexp & Escape",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": {
        "foreground": "#c7d4e2"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ce6f65"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.bracket",
        "meta.delimiter",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#e3e0dc"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e0a46a"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#e3e0dc"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#c2cdda"
      }
    },
    {
      "name": "Classes, Types, Interfaces",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d6b56f"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface", "support.type.interface"],
      "settings": {
        "foreground": "#c2cdda"
      }
    },
    {
      "name": "Enum & Enum Members",
      "scope": [
        "entity.name.type.enum",
        "variable.other.enummember",
        "constant.other.enummember"
      ],
      "settings": {
        "foreground": "#e8b860"
      }
    },
    {
      "name": "Properties & Fields",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.property.object",
        "meta.object-literal.key",
        "meta.member"
      ],
      "settings": {
        "foreground": "#c7d4e2"
      }
    },
    {
      "name": "Decorators / Attributes",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js",
        "meta.decorator",
        "meta.attribute",
        "entity.name.attribute",
        "entity.name.function.decorator",
        "variable.function.decorator"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d6b56f"
      }
    },
    {
      "name": "Language Builtins",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super",
        "variable.language.self",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ce6f65"
      }
    },
    {
      "name": "[TS/JS] Functions",
      "scope": [
        "source.ts entity.name.function",
        "source.tsx entity.name.function",
        "source.js entity.name.function",
        "source.jsx entity.name.function",
        "source.ts entity.name.method",
        "source.tsx entity.name.method",
        "source.js entity.name.method",
        "source.jsx entity.name.method"
      ],
      "settings": {
        "foreground": "#e0a46a"
      }
    },
    {
      "name": "[TS/JS] Parameters",
      "scope": [
        "source.ts variable.parameter",
        "source.tsx variable.parameter",
        "source.js variable.parameter",
        "source.jsx variable.parameter"
      ],
      "settings": {
        "foreground": "#c2cdda"
      }
    },
    {
      "name": "[TS/JS] Properties",
      "scope": [
        "source.ts variable.other.property",
        "source.ts meta.object-literal.key",
        "source.js variable.other.property",
        "source.js meta.object-literal.key",
        "support.variable.property.js",
        "support.variable.property.ts"
      ],
      "settings": {
        "foreground": "#c7d4e2"
      }
    },
    {
      "name": "[TS/JS] Classes & Types",
      "scope": [
        "source.ts entity.name.type",
        "source.ts entity.name.class",
        "source.tsx entity.name.type",
        "source.tsx entity.name.class",
        "source.ts entity.name.type.interface",
        "source.tsx entity.name.type.interface",
        "source.ts entity.name.type.enum",
        "source.tsx entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#d6b56f"
      }
    },
    {
      "name": "[TS/JS] Interfaces",
      "scope": [
        "source.ts entity.name.type.interface",
        "source.tsx entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#c2cdda"
      }
    },
    {
      "name": "[TS/JS] Type Aliases",
      "scope": [
        "source.ts keyword.other.type-alias",
        "source.tsx keyword.other.type-alias"
      ],
      "settings": {
        "foreground": "#d6b56f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[TS/JS] Type Annotations",
      "scope": [
        "source.ts meta.type.annotation",
        "source.tsx meta.type.annotation",
        "source.ts storage.type",
        "source.tsx storage.type"
      ],
      "settings": {
        "foreground": "#d6b56f"
      }
    },
    {
      "name": "[JSX/TSX] Tags & Attributes",
      "scope": [
        "entity.name.tag.tsx",
        "entity.name.tag.jsx",
        "meta.tag.tsx",
        "meta.tag.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.jsx"
      ],
      "settings": {
        "foreground": "#ce6f65"
      }
    },
    {
      "name": "[TS/JS] React Hooks",
      "scope": [
        "source.ts variable.function",
        "source.tsx variable.function",
        "source.js variable.function",
        "source.jsx variable.function"
      ],
      "settings": {
        "foreground": "#e0a46a"
      }
    },
    {
      "name": "[TS/JS] Decorators",
      "scope": ["source.ts meta.decorator", "source.tsx meta.decorator"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d6b56f"
      }
    },
    {
      "name": "[Python] Functions",
      "scope": [
        "source.python entity.name.function",
        "source.python meta.function-call",
        "source.python support.function"
      ],
      "settings": {
        "foreground": "#e0a46a"
      }
    },
    {
      "name": "[Python] Parameters",
      "scope": [
        "source.python variable.parameter.function",
        "source.python variable.parameter"
      ],
      "settings": {
        "foreground": "#c2cdda"
      }
    },
    {
      "name": "[Python] Classes & Types",
      "scope": [
        "source.python entity.name.class",
        "source.python support.type",
        "source.python support.class"
      ],
      "settings": {
        "foreground": "#d6b56f"
      }
    },
    {
      "name": "[Python] Builtins",
      "scope": [
        "source.python support.function.builtin",
        "source.python support.type.builtin",
        "constant.language.python"
      ],
      "settings": {
        "foreground": "#e2c48f"
      }
    },
    {
      "name": "[Python] self",
      "scope": ["source.python variable.language.self"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ce6f65"
      }
    },
    {
