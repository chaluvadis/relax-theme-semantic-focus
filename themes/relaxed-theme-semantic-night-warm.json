    "semanticTokenColors": {
        "comment": {
            "foreground": "#908a82",
            "fontStyle": "italic"
        },
        "keyword": "#a6789f",
        "string": "#95ad63",
        "function": "#8fa0a0",
        "function.declaration": "#9fb0b0",
        "method": "#8fa0a0",
        "method.declaration": "#9fb0b0",
        "property": "#c7d4e2",
        "property.readonly": "#e2c48f",
        "type": "#d6b56f",
        "class": "#d6b56f",
        "interface": "#c2cdda",
        "enum": "#d6b56f",
        "enumMember": "#e2c48f",
        "typeParameter": {
            "foreground": "#c7d4e2",
            "fontStyle": "italic"
        },
        "parameter": "#c7d4e2",
        "parameter.readonly": "#c7d4e2",
        "variable": "#e3e0dc",
        "variable.readonly": "#e2c48f",
        "variable.constant": "#e2c48f",
        "variable.global": "#e3e0dc",
        "variable.defaultLibrary": {
            "foreground": "#c7d4e2",
            "fontStyle": "italic"
        },
        "event": "#a6789f",
        "macro": "#a6789f",
        "decorator": {
            "foreground": "#8fa0a0",
            "fontStyle": "italic"
        },
        "label": "#a19a91",
        "builtinType": "#d6b56f",
        "operator": "#e3e0dc",
        "string.regexp": "#c7d4e2",
        "formatSpecifier": "#c7d4e2",
        "escape": "#c7d4e2",
        "number": "#e2c48f",
        "boolean": "#ce6f65",
        "typeAlias": "#d6b56f"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#908a82"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#a6789f"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#95ad63"
            }
        },
        {
            "name": "Numbers & Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#e2c48f"
            }
        },
        {
            "name": "Regexp & Escape",
            "scope": [
                "string.regexp",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ce6f65"
            }
        },
        {
            "name": "Operators & Punctuation",
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.bracket",
                "meta.delimiter",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#e3e0dc"
            }
        },
        {
            "name": "Functions & Methods",
            "scope": [
                "entity.name.function",
                "entity.name.method",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other.readwrite",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#e3e0dc"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "meta.parameter"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "Classes, Types, Interfaces",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#d6b56f"
            }
        },
        {
            "name": "Interfaces",
            "scope": [
                "entity.name.type.interface",
                "support.type.interface"
            ],
            "settings": {
                "foreground": "#c2cdda"
            }
        },
        {
            "name": "Enum & Enum Members",
            "scope": [
                "entity.name.type.enum",
                "variable.other.enummember",
                "constant.other.enummember"
            ],
            "settings": {
                "foreground": "#e2c48f"
            }
        },
        {
            "name": "Properties & Fields",
            "scope": [
                "variable.other.property",
                "variable.other.object.property",
                "support.variable.property",
                "meta.property.object",
                "meta.object-literal.key",
                "meta.member"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "Decorators / Attributes",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js",
                "meta.decorator",
                "meta.attribute",
                "entity.name.attribute",
                "entity.name.function.decorator",
                "variable.function.decorator"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "Language Builtins",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.super",
                "variable.language.self",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ce6f65"
            }
        },
        {
            "name": "[TS/JS] Functions",
            "scope": [
                "source.ts entity.name.function",
                "source.tsx entity.name.function",
                "source.js entity.name.function",
                "source.jsx entity.name.function",
                "source.ts entity.name.method",
                "source.tsx entity.name.method",
                "source.js entity.name.method",
                "source.jsx entity.name.method"
            ],
            "settings": {
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "[TS/JS] Parameters",
            "scope": [
                "source.ts variable.parameter",
                "source.tsx variable.parameter",
                "source.js variable.parameter",
                "source.jsx variable.parameter"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "[TS/JS] Properties",
            "scope": [
                "source.ts variable.other.property",
                "source.ts meta.object-literal.key",
                "source.js variable.other.property",
                "source.js meta.object-literal.key",
                "support.variable.property.js",
                "support.variable.property.ts"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "[TS/JS] Classes & Types",
            "scope": [
                "source.ts entity.name.type",
                "source.ts entity.name.class",
                "source.tsx entity.name.type",
                "source.tsx entity.name.class",
                "source.ts entity.name.type.interface",
                "source.tsx entity.name.type.interface",
                "source.ts entity.name.type.enum",
                "source.tsx entity.name.type.enum"
            ],
            "settings": {
                "foreground": "#d6b56f"
            }
        },
        {
            "name": "[TS/JS] Interfaces",
            "scope": [
                "source.ts entity.name.type.interface",
                "source.tsx entity.name.type.interface"
            ],
            "settings": {
                "foreground": "#c2cdda"
            }
        },
        {
            "name": "[TS/JS] Type Annotations",
            "scope": [
                "source.ts meta.type.annotation",
                "source.tsx meta.type.annotation",
                "source.ts storage.type",
                "source.tsx storage.type"
            ],
            "settings": {
                "foreground": "#d6b56f"
            }
        },
        {
            "name": "[JSX/TSX] Tags & Attributes",
            "scope": [
                "entity.name.tag.tsx",
                "entity.name.tag.jsx",
                "meta.tag.tsx",
                "meta.tag.jsx",
                "entity.other.attribute-name.tsx",
                "entity.other.attribute-name.jsx"
            ],
            "settings": {
                "foreground": "#ce6f65"
            }
        },
        {
            "name": "[TS/JS] Decorators",
            "scope": [
                "source.ts meta.decorator",
                "source.tsx meta.decorator"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "[Python] Functions",
            "scope": [
                "source.python entity.name.function",
                "source.python meta.function-call",
                "source.python support.function"
            ],
            "settings": {
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "[Python] Parameters",
            "scope": [
                "source.python variable.parameter.function",
                "source.python variable.parameter"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "[Python] Classes & Types",
            "scope": [
                "source.python entity.name.class",
                "source.python support.type",
                "source.python support.class"
            ],
            "settings": {
                "foreground": "#d6b56f"
            }
        },
        {
            "name": "[Python] Builtins",
            "scope": [
                "source.python support.function.builtin",
                "source.python support.type.builtin",
                "constant.language.python"
            ],
            "settings": {
                "foreground": "#e2c48f"
            }
        },
        {
            "name": "[Python] self",
            "scope": [
                "source.python variable.language.self"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ce6f65"
            }
        },
        {
            "name": "[C#] Methods",
            "scope": [
                "source.cs entity.name.method",
                "source.cs meta.method-call",
                "source.cs support.function"
            ],
            "settings": {
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "[C#] Parameters",
            "scope": [
                "source.cs variable.parameter",
                "source.cs meta.parameter"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "[C#] Primary Constructor Parameters",
            "scope": [
                "source.cs variable.parameter.primary-constructor"
            ],
            "settings": {
                "foreground": "#e5c287"
            }
        },
        {
            "name": "[C#] Classes, Structs, Interfaces, Enums",
            "scope": [
                "source.cs entity.name.type",
                "source.cs entity.name.class",
                "source.cs entity.name.struct",
                "source.cs entity.name.interface",
                "source.cs entity.name.enum",
                "source.cs support.type",
                "source.cs support.class"
            ],
            "settings": {
                "foreground": "#d6b56f"
            }
        },
        {
            "name": "[C#] Namespace",
            "scope": [
                "source.cs entity.name.namespace",
                "source.cs support.namespace"
            ],
            "settings": {
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "[C#] Attributes (Annotations)",
            "scope": [
                "source.cs meta.attribute",
                "source.cs entity.name.attribute"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8fa0a0"
            }
        },
        {
            "name": "[C#] Keywords",
            "scope": [
                "source.cs keyword",
                "source.cs storage.type",
                "source.cs storage.modifier"
            ],
            "settings": {
                "foreground": "#a6789f"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#d6b56f"
            }
        },
        {
            "name": "JSON Strings",
            "scope": [
                "source.json string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#c7d4e2"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": [
                "markup.heading",
                "markdown.heading",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#95ad63"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.title.markdown",
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#9a8f7a"
            }
        }
    ]
}