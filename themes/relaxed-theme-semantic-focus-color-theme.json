	"semanticTokenColors": {
		"namespace": "#8aa0b3",
		"module": "#8aa0b3",
		"type": "#e7c380",
		"class": "#e7c380",
		"struct": "#e7c380",
		"interface": "#acbbd0",
		"enum": "#e7c380",
		"enumMember": "#ecd3a8",
		"typeParameter": {
			"foreground": "#c9dfff",
			"fontStyle": "italic"
		},
		"parameter": "#acbbd0",
		"parameter.readonly": "#acbbd0",
		"variable": "#e0e0e0",
		"variable.readonly": "#ecd3a8",
		"variable.constant": "#ecd3a8",
		"variable.global": "#e0e0e0",
		"variable.defaultLibrary": {
			"foreground": "#c9dfff",
			"fontStyle": "italic"
		},
		"property": "#c9dfff",
		"property.readonly": "#ecd3a8",
		"property.static": "#c9dfff",
		"event": "#b06698",
		"function": "#6a8799",
		"function.declaration": "#7eaac7",
		"method": "#6a8799",
		"method.declaration": "#7eaac7",
		"macro": "#b06698",
		"decorator": {
			"foreground": "#6a8799",
			"fontStyle": "italic"
		},
		"label": "#7a7a7a",
		"builtinType": "#e7c380",
		"keyword": "#b06698",
		"operator": "#e0e0e0",
		"comment": {
			"foreground": "#7a7a7a",
			"fontStyle": "italic"
		},
		"string": "#97af5d",
		"character": "#97af5d",
		"number": "#ecd3a8",
		"boolean": "#d1635e",
		"regexp": "#acbbd0",
		"formatSpecifier": "#acbbd0",
		"escape": "#acbbd0",
		"typeAlias": "#e7c380"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7a7a7a"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#b06698"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted",
				"string.template"
			],
			"settings": {
				"foreground": "#97af5d"
			}
		},
		{
			"name": "Numbers & Constants",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.language",
				"support.constant"
			],
			"settings": {
				"foreground": "#ecd3a8"
			}
		},
		{
			"name": "Regexp & Escape",
			"scope": [
				"string.regexp",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#acbbd0"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#d1635e"
			}
		},
		{
			"name": "Operators & Punctuation",
			"scope": [
				"punctuation",
				"meta.brace",
				"meta.bracket",
				"meta.delimiter",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#e0e0e0"
			}
		},
		{
			"name": "Functions & Methods",
			"scope": [
				"entity.name.function",
				"entity.name.method",
				"meta.function-call",
				"support.function"
			],
			"settings": {
				"foreground": "#6a8799"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other.readwrite",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#e0e0e0"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter",
				"meta.parameter"
			],
			"settings": {
				"foreground": "#acbbd0"
			}
		},
		{
			"name": "Classes, Types, Interfaces",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"support.class",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#e7c380"
			}
		},
		{
			"name": "Interfaces",
			"scope": [
				"entity.name.type.interface",
				"support.type.interface"
			],
			"settings": {
				"foreground": "#acbbd0"
			}
		},
		{
			"name": "Enum & Enum Members",
			"scope": [
				"entity.name.type.enum",
				"variable.other.enummember",
				"constant.other.enummember"
			],
			"settings": {
				"foreground": "#ecd3a8"
			}
		},
		{
			"name": "Properties & Fields",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.variable.property",
				"meta.property.object",
				"meta.object-literal.key",
				"meta.member"
			],
			"settings": {
				"foreground": "#c9dfff"
			}
		},
		{
			"name": "Decorators / Attributes",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js",
				"meta.decorator",
				"meta.attribute",
				"entity.name.attribute",
				"entity.name.function.decorator",
				"variable.function.decorator"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6a8799"
			}
		},
		{
			"name": "Language Builtins",
			"scope": [
				"variable.language",
				"variable.language.this",
				"variable.language.super",
				"variable.language.self",
				"constant.language.null",
				"constant.language.undefined"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d1635e"
			}
		},
		{
			"name": "[TS/JS] Functions",
			"scope": [
				"source.ts entity.name.function",
				"source.tsx entity.name.function",
				"source.js entity.name.function",
				"source.jsx entity.name.function",
				"source.ts entity.name.method",
				"source.tsx entity.name.method",
				"source.js entity.name.method",
				"source.jsx entity.name.method"
			],
			"settings": {
				"foreground": "#6a8799"
			}
		},
		{
			"name": "[TS/JS] Parameters",
			"scope": [
				"source.ts variable.parameter",
				"source.tsx variable.parameter",
				"source.js variable.parameter",
				"source.jsx variable.parameter"
			],
			"settings": {
				"foreground": "#acbbd0"
			}
		},
		{
			"name": "[TS/JS] Properties",
			"scope": [
				"source.ts variable.other.property",
