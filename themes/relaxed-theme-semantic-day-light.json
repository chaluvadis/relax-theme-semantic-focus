    {
      "name": "[TS/JS] Decorators",
      "scope": ["source.ts meta.decorator", "source.tsx meta.decorator"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2f6d86"
      }
    },
    {
      "name": "[Python] Functions",
      "scope": [
        "source.python entity.name.function",
        "source.python meta.function-call",
        "source.python support.function"
      ],
      "settings": {
        "foreground": "#2f6d86"
      }
    },
    {
      "name": "[Python] Parameters",
      "scope": [
