  "semanticTokenColors": {
    "comment": {
      "foreground": "#6a6f7a",
      "fontStyle": "italic"
    },
    "keyword": "#7a4f7f",
    "string": "#4b7d53",
    "function": {
      "foreground": "#2f6d86",
      "fontStyle": "bold"
    },
    "function.declaration": "#2f6d86",
    "method": {
      "foreground": "#2f6d86",
      "fontStyle": "bold"
    },
    "method.declaration": "#2f6d86",
    "property": "#3f5f8f",
    "property.readonly": "#7a5f22",
    "type": "#8b6f2e",
    "class": {
      "foreground": "#8b6f2e",
      "fontStyle": "bold"
    },
    "interface": "#556c82",
    "enum": "#8b6f2e",
    "enumMember": "#a08f52",
    "typeParameter": {
      "foreground": "#3f5f8f",
      "fontStyle": "italic"
    },
    "parameter": "#556c82",
    "parameter.readonly": "#9a7f42",
    "variable": "#2a2f37",
    "variable.readonly": "#7a5f22",
    "variable.constant": "#7a5f22",
    "variable.global": "#2a2f37",
    "variable.defaultLibrary": {
      "foreground": "#3f5f8f",
      "fontStyle": "italic"
    },
    "event": "#7a4f7f",
    "macro": "#7a4f7f",
    "decorator": {
      "foreground": "#2f6d86",
      "fontStyle": "italic"
    },
    "keyword.decorator": {
      "foreground": "#2f6d86",
      "fontStyle": "italic"
    },
    "label": "#5b6472",
    "builtinType": "#8b6f2e",
    "operator": "#2a2f37",
    "string.regexp": "#3f5f8f",
    "formatSpecifier": "#3f5f8f",
    "escape": "#3f5f8f",
    "number": "#9a7f42",
    "boolean": "#b03e34",
    "typeAlias": "#8b6f2e",
    "namespace": "#2f6d86",
    "struct": "#8b6f2e",
    "modifier": "#7a4f7f"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6a6f7a"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#7a4f7f"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#4b7d53"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#7a5f22"
      }
    },
    {
      "name": "Regexp & Escape",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": {
        "foreground": "#3f5f8f"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#b03e34"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.bracket",
        "meta.delimiter",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#2a2f37"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2f6d86"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#2a2f37"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#556c82"
      }
    },
    {
      "name": "Classes, Types, Interfaces",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8b6f2e"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface", "support.type.interface"],
      "settings": {
        "foreground": "#556c82"
      }
    },
    {
      "name": "Enum & Enum Members",
      "scope": [
        "entity.name.type.enum",
        "variable.other.enummember",
        "constant.other.enummember"
      ],
      "settings": {
        "foreground": "#a08f52"
      }
    },
    {
      "name": "Properties & Fields",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.property.object",
        "meta.object-literal.key",
        "meta.member"
      ],
      "settings": {
        "foreground": "#3f5f8f"
      }
    },
    {
      "name": "Decorators / Attributes",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js",
        "meta.decorator",
        "meta.attribute",
        "entity.name.attribute",
        "entity.name.function.decorator",
        "variable.function.decorator"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2f6d86"
      }
    },
    {
      "name": "Language Builtins",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super",
        "variable.language.self",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b03e34"
      }
    },
    {
      "name": "[TS/JS] Functions",
      "scope": [
        "source.ts entity.name.function",
        "source.tsx entity.name.function",
        "source.js entity.name.function",
        "source.jsx entity.name.function",
        "source.ts entity.name.method",
        "source.tsx entity.name.method",
        "source.js entity.name.method",
        "source.jsx entity.name.method"
      ],
      "settings": {
        "foreground": "#2f6d86"
      }
    },
    {
      "name": "[TS/JS] Parameters",
