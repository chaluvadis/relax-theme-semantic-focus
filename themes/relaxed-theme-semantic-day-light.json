    "semanticTokenColors": {
        "comment": {
            "foreground": "#6a6f7a",
            "fontStyle": "italic"
        },
        "keyword": "#7a4f7f",
        "string": "#4b7d53",
        "function": "#2f6d86",
        "function.declaration": "#3e7c96",
        "method": "#2f6d86",
        "method.declaration": "#3e7c96",
        "property": "#3f5f8f",
        "property.readonly": "#7a5f22",
        "type": "#8b6f2e",
        "class": "#8b6f2e",
        "interface": "#556c82",
        "enum": "#8b6f2e",
        "enumMember": "#7a5f22",
        "typeParameter": {
            "foreground": "#3f5f8f",
            "fontStyle": "italic"
        },
        "parameter": "#556c82",
        "parameter.readonly": "#556c82",
        "variable": "#2a2f37",
        "variable.readonly": "#7a5f22",
        "variable.constant": "#7a5f22",
        "variable.global": "#2a2f37",
        "variable.defaultLibrary": {
            "foreground": "#3f5f8f",
            "fontStyle": "italic"
        },
        "event": "#7a4f7f",
        "macro": "#7a4f7f",
        "decorator": {
            "foreground": "#2f6d86",
            "fontStyle": "italic"
        },
        "label": "#5b6472",
        "builtinType": "#8b6f2e",
        "operator": "#2a2f37",
        "string.regexp": "#3f5f8f",
        "formatSpecifier": "#3f5f8f",
        "escape": "#3f5f8f",
        "number": "#7a5f22",
        "boolean": "#b03e34",
        "typeAlias": "#8b6f2e"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6a6f7a"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#7a4f7f"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#4b7d53"
            }
        },
        {
            "name": "Numbers & Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#7a5f22"
            }
        },
        {
            "name": "Regexp & Escape",
            "scope": [
                "string.regexp",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#3f5f8f"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#b03e34"
            }
        },
        {
            "name": "Operators & Punctuation",
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.bracket",
                "meta.delimiter",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#2a2f37"
            }
        },
        {
            "name": "Functions & Methods",
            "scope": [
                "entity.name.function",
                "entity.name.method",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other.readwrite",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#2a2f37"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "meta.parameter"
            ],
            "settings": {
                "foreground": "#556c82"
            }
        },
        {
            "name": "Classes, Types, Interfaces",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#8b6f2e"
            }
        },
        {
            "name": "Interfaces",
            "scope": [
                "entity.name.type.interface",
                "support.type.interface"
            ],
            "settings": {
                "foreground": "#556c82"
            }
        },
        {
            "name": "Enum & Enum Members",
            "scope": [
                "entity.name.type.enum",
                "variable.other.enummember",
                "constant.other.enummember"
            ],
            "settings": {
                "foreground": "#7a5f22"
            }
        },
        {
            "name": "Properties & Fields",
            "scope": [
                "variable.other.property",
                "variable.other.object.property",
                "support.variable.property",
                "meta.property.object",
                "meta.object-literal.key",
                "meta.member"
            ],
            "settings": {
                "foreground": "#3f5f8f"
            }
        },
        {
            "name": "Decorators / Attributes",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js",
                "meta.decorator",
                "meta.attribute",
                "entity.name.attribute",
                "entity.name.function.decorator",
                "variable.function.decorator"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "Language Builtins",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.super",
                "variable.language.self",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#b03e34"
            }
        },
        {
            "name": "[TS/JS] Functions",
            "scope": [
                "source.ts entity.name.function",
                "source.tsx entity.name.function",
                "source.js entity.name.function",
                "source.jsx entity.name.function",
                "source.ts entity.name.method",
                "source.tsx entity.name.method",
                "source.js entity.name.method",
                "source.jsx entity.name.method"
            ],
            "settings": {
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "[TS/JS] Parameters",
            "scope": [
                "source.ts variable.parameter",
                "source.tsx variable.parameter",
                "source.js variable.parameter",
                "source.jsx variable.parameter"
            ],
            "settings": {
                "foreground": "#556c82"
            }
        },
        {
            "name": "[TS/JS] Properties",
            "scope": [
                "source.ts variable.other.property",
                "source.ts meta.object-literal.key",
                "source.js variable.other.property",
                "source.js meta.object-literal.key",
                "support.variable.property.js",
                "support.variable.property.ts"
            ],
            "settings": {
                "foreground": "#3f5f8f"
            }
        },
        {
            "name": "[TS/JS] Classes & Types",
            "scope": [
                "source.ts entity.name.type",
                "source.ts entity.name.class",
                "source.tsx entity.name.type",
                "source.tsx entity.name.class",
                "source.ts entity.name.type.interface",
                "source.tsx entity.name.type.interface",
                "source.ts entity.name.type.enum",
                "source.tsx entity.name.type.enum"
            ],
            "settings": {
                "foreground": "#8b6f2e"
            }
        },
        {
            "name": "[TS/JS] Interfaces",
            "scope": [
                "source.ts entity.name.type.interface",
                "source.tsx entity.name.type.interface"
            ],
            "settings": {
                "foreground": "#556c82"
            }
        },
        {
            "name": "[TS/JS] Type Annotations",
            "scope": [
                "source.ts meta.type.annotation",
                "source.tsx meta.type.annotation",
                "source.ts storage.type",
                "source.tsx storage.type"
            ],
            "settings": {
                "foreground": "#8b6f2e"
            }
        },
        {
            "name": "[JSX/TSX] Tags & Attributes",
            "scope": [
                "entity.name.tag.tsx",
                "entity.name.tag.jsx",
                "meta.tag.tsx",
                "meta.tag.jsx",
                "entity.other.attribute-name.tsx",
                "entity.other.attribute-name.jsx"
            ],
            "settings": {
                "foreground": "#b03e34"
            }
        },
        {
            "name": "[TS/JS] Decorators",
            "scope": [
                "source.ts meta.decorator",
                "source.tsx meta.decorator"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "[Python] Functions",
            "scope": [
                "source.python entity.name.function",
                "source.python meta.function-call",
                "source.python support.function"
            ],
            "settings": {
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "[Python] Parameters",
            "scope": [
                "source.python variable.parameter.function",
                "source.python variable.parameter"
            ],
            "settings": {
                "foreground": "#556c82"
            }
        },
        {
            "name": "[Python] Classes & Types",
            "scope": [
                "source.python entity.name.class",
                "source.python support.type",
                "source.python support.class"
            ],
            "settings": {
                "foreground": "#8b6f2e"
            }
        },
        {
            "name": "[Python] Builtins",
            "scope": [
                "source.python support.function.builtin",
                "source.python support.type.builtin",
                "constant.language.python"
            ],
            "settings": {
                "foreground": "#7a5f22"
            }
        },
        {
            "name": "[Python] self",
            "scope": [
                "source.python variable.language.self"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#b03e34"
            }
        },
        {
            "name": "[C#] Methods",
            "scope": [
                "source.cs entity.name.method",
                "source.cs meta.method-call",
                "source.cs support.function"
            ],
            "settings": {
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "[C#] Parameters",
            "scope": [
                "source.cs variable.parameter",
                "source.cs meta.parameter"
            ],
            "settings": {
                "foreground": "#556c82"
            }
        },
        {
            "name": "[C#] Primary Constructor Parameters",
            "scope": [
                "source.cs variable.parameter.primary-constructor"
            ],
            "settings": {
                "foreground": "#e5c287"
            }
        },
        {
            "name": "[C#] Classes, Structs, Interfaces, Enums",
            "scope": [
                "source.cs entity.name.type",
                "source.cs entity.name.class",
                "source.cs entity.name.struct",
                "source.cs entity.name.interface",
                "source.cs entity.name.enum",
                "source.cs support.type",
                "source.cs support.class"
            ],
            "settings": {
                "foreground": "#8b6f2e"
            }
        },
        {
            "name": "[C#] Namespace",
            "scope": [
                "source.cs entity.name.namespace",
                "source.cs support.namespace"
            ],
            "settings": {
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "[C#] Attributes (Annotations)",
            "scope": [
                "source.cs meta.attribute",
                "source.cs entity.name.attribute"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#2f6d86"
            }
        },
        {
            "name": "[C#] Keywords",
            "scope": [
                "source.cs keyword",
                "source.cs storage.type",
                "source.cs storage.modifier"
            ],
            "settings": {
                "foreground": "#7a4f7f"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#8b6f2e"
            }
        },
        {
            "name": "JSON Strings",
            "scope": [
                "source.json string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#3f5f8f"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": [
                "markup.heading",
                "markdown.heading",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#4b7d53"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.title.markdown",
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#4b6b7d"
            }
        }
    ]
}