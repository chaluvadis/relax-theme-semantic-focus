    {
      "name": "Readonly Properties (TS)",
      "scope": [
        "variable.other.property.readonly",
        "meta.property.readonly",
        "storage.modifier.readonly"
      ],
      "settings": { "foreground": "#7a5f22", "fontStyle": "" }
    }
  ]
