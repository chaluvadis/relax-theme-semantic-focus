    "list.focusForeground": "#2a2f37",
    "list.inactiveSelectionForeground": "#2a2f37",
    "list.highlightForeground": "#4b6b7d",
    "editorSuggestWidget.highlightForeground": "#4b6b7d",
    "textLink.foreground": "#4b6b7d",
    "progressBar.background": "#4b6b7d",
    "pickerGroup.foreground": "#4b6b7d",
    "notificationLink.foreground": "#4b6b7d",
    "input.border": "#4b6b7d",
    "terminal.background": "#eaedf1",
    "terminal.foreground": "#2a2f37",
    "terminal.ansiBlack": "#4a4a4a",
    "terminal.ansiBlue": "#2f6d86",
    "terminal.ansiBrightBlack": "#7a7f87",
    "terminal.ansiBrightBlue": "#2f6d86",
    "terminal.ansiBrightCyan": "#3f5f8f",
    "terminal.ansiBrightGreen": "#4b7d53",
    "terminal.ansiBrightMagenta": "#7a4f7f",
    "terminal.ansiBrightRed": "#b03e34",
    "terminal.ansiBrightWhite": "#2a2f37",
    "terminal.ansiBrightYellow": "#8b6f2e",
    "terminal.ansiCyan": "#3f5f8f",
    "terminal.ansiGreen": "#4b7d53",
    "terminal.ansiMagenta": "#7a4f7f",
    "terminal.ansiRed": "#b03e34",
    "terminal.ansiWhite": "#2a2f37",
    "terminal.ansiYellow": "#8b6f2e",
    "terminal.selectionBackground": "#4b6b7d55",
    "terminalCursor.background": "#2a2f37",
    "editorSuggestWidget.background": "#e9ebef",
    "editorSuggestWidget.foreground": "#2a2f37",
    "editorSuggestWidget.border": "#d9dee5",
    "editorSuggestWidget.selectedBackground": "#e6eaee",
    "editorSuggestWidget.selectedForeground": "#2a2f37"
  },
  "semanticTokenColors": {
    "property": { "foreground": "#4b6b7d" },
    "method": { "foreground": "#8b6f2e" },
    "function": { "foreground": "#8b6f2e" },
    "variable": { "foreground": "#2a2f37" },
    "variable.readonly": { "foreground": "#7a4f7f" },
    "class": { "foreground": "#2f6d86" },
    "type": { "foreground": "#2f6d86" },
    "interface": { "foreground": "#7a4f7f" },
    "typeParameter": { "foreground": "#4b6b7d" }
  },
  "tokenColors": [
    {
      "scope": [
        "variable.other.member",
        "variable.other.property",
        "variable.property"
      ],
      "settings": { "foreground": "#4b6b7d" }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "meta.method-call"
      ],
      "settings": { "foreground": "#8b6f2e" }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.readonly",
        "constant.other"
      ],
      "settings": { "foreground": "#7a4f7f" }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "support.class",
        "entity.name.type.interface"
      ],
      "settings": { "foreground": "#2f6d86" }
    }
  ]
}
