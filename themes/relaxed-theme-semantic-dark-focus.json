  "semanticTokenColors": {
    "comment": {
      "foreground": "#8a9098",
      "fontStyle": "italic"
    },
    "keyword": "#a47aa7",
    "string": "#8fae6b",
    "function": {
      "foreground": "#85a7bf",
      "fontStyle": "bold"
    },
    "function.declaration": "#85a7bf",
    "method": {
      "foreground": "#85a7bf",
      "fontStyle": "bold"
    },
    "method.declaration": "#9ab4c6",
    "property": "#b9d3ea",
    "property.readonly": "#e4a85c",
    "type": "#d7c27e",
    "class": {
      "foreground": "#d7c27e",
      "fontStyle": "bold"
    },
    "interface": "#b8c8d8",
    "enum": "#d7c27e",
    "enumMember": "#e8b85f",
    "typeParameter": {
      "foreground": "#b9d3ea",
      "fontStyle": "italic"
    },
    "parameter": "#b8c8d8",
    "parameter.readonly": "#ecd3a8",
    "variable": "#dde1e6",
    "variable.readonly": "#e4a85c",
    "variable.constant": "#b8854a",
    "variable.global": "#dde1e6",
    "variable.defaultLibrary": {
      "foreground": "#b9d3ea",
      "fontStyle": "italic"
    },
    "event": "#a47aa7",
    "macro": "#a47aa7",
    "decorator": {
      "foreground": "#85a7bf",
      "fontStyle": "italic"
    },
    "keyword.decorator": {
      "foreground": "#85a7bf",
      "fontStyle": "italic"
    },
    "label": "#9aa1aa",
    "builtinType": "#d7c27e",
    "operator": "#dde1e6",
    "string.regexp": "#b9d3ea",
    "formatSpecifier": "#b9d3ea",
    "escape": "#b9d3ea",
    "number": "#e4a85c",
    "boolean": "#c4776f",
    "typeAlias": "#d7c27e",
    "namespace": "#8aa0b3",
    "struct": "#d7c27e",
    "modifier": "#a47aa7"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8a9098"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#a47aa7"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#8fae6b"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#b8854a"
      }
    },
    {
      "name": "Regexp & Escape",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": {
        "foreground": "#b9d3ea"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#c4776f"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.bracket",
        "meta.delimiter",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#dde1e6"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#85a7bf"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#dde1e6"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#b8c8d8"
      }
    },
    {
      "name": "Classes, Types, Interfaces",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d7c27e"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface", "support.type.interface"],
      "settings": {
        "foreground": "#b8c8d8"
      }
    },
    {
      "name": "Enum & Enum Members",
      "scope": [
        "entity.name.type.enum",
        "variable.other.enummember",
        "constant.other.enummember"
      ],
      "settings": {
        "foreground": "#e8b85f"
      }
    },
    {
      "name": "Properties & Fields",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.property.object",
        "meta.object-literal.key",
        "meta.member"
      ],
      "settings": {
        "foreground": "#b9d3ea"
      }
    },
    {
      "name": "Decorators / Attributes",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js",
        "meta.decorator",
        "meta.attribute",
        "entity.name.attribute",
        "entity.name.function.decorator",
        "variable.function.decorator"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#85a7bf"
      }
    },
    {
      "name": "Language Builtins",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super",
        "variable.language.self",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c4776f"
      }
    },
    {
      "name": "[TS/JS] Functions",
      "scope": [
        "source.ts entity.name.function",
        "source.tsx entity.name.function",
        "source.js entity.name.function",
        "source.jsx entity.name.function",
        "source.ts entity.name.method",
        "source.tsx entity.name.method",
        "source.js entity.name.method",
        "source.jsx entity.name.method"
      ],
      "settings": {
        "foreground": "#85a7bf"
      }
    },
    {
      "name": "[TS/JS] Parameters",
      "scope": [
        "source.ts variable.parameter",
        "source.tsx variable.parameter",
        "source.js variable.parameter",
        "source.jsx variable.parameter"
      ],
      "settings": {
        "foreground": "#b8c8d8"
      }
    },
    {
      "name": "[TS/JS] Properties",
      "scope": [
        "source.ts variable.other.property",
        "source.ts meta.object-literal.key",
        "source.js variable.other.property",
        "source.js meta.object-literal.key",
        "support.variable.property.js",
        "support.variable.property.ts"
