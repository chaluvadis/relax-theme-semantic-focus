  "semanticTokenColors": {
    "property": { "foreground": "#7e8f9c" },
    "method": { "foreground": "#d7c27e" },
    "function": { "foreground": "#d7c27e" },
    "variable": { "foreground": "#dde1e6" },
    "variable.readonly": { "foreground": "#a47aa7" },
    "class": { "foreground": "#85a7bf" },
    "type": { "foreground": "#85a7bf" },
    "interface": { "foreground": "#a47aa7" },
    "typeParameter": { "foreground": "#7e8f9c" }
  },
  "tokenColors": [
    {
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.interface.cs"
      ],
      "settings": { "foreground": "#a47aa7" }
    },
    {
      "scope": [
        "entity.name.type.class",
        "entity.name.type.class.cs"
      ],
      "settings": { "foreground": "#85a7bf" }
    },
    {
      "scope": [
        "variable.other.member",
        "variable.other.property",
        "variable.property"
      ],
      "settings": { "foreground": "#7e8f9c" }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.method-call",
        "meta.function-call.cs"
      ],
      "settings": { "foreground": "#d7c27e" }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.readonly",
        "constant.other"
      ],
      "settings": { "foreground": "#a47aa7" }
    }
  ]
}
