    "semanticTokenColors": {
        "comment": {
            "foreground": "#8a9098",
            "fontStyle": "italic"
        },
        "keyword": "#a47aa7",
        "string": "#8fae6b",
        "function": "#85a7bf",
        "function.declaration": "#9ab4c6",
        "method": "#85a7bf",
        "method.declaration": "#9ab4c6",
        "property": "#b9d3ea",
        "property.readonly": "#dfc39a",
        "type": "#d7c27e",
        "class": "#d7c27e",
        "interface": "#b8c8d8",
        "enum": "#d7c27e",
        "enumMember": "#dfc39a",
        "typeParameter": {
            "foreground": "#b9d3ea",
            "fontStyle": "italic"
        },
        "parameter": "#b9d3ea",
        "parameter.readonly": "#b9d3ea",
        "variable": "#dde1e6",
        "variable.readonly": "#dfc39a",
        "variable.constant": "#dfc39a",
        "variable.global": "#dde1e6",
        "variable.defaultLibrary": {
            "foreground": "#b9d3ea",
            "fontStyle": "italic"
        },
        "event": "#a47aa7",
        "macro": "#a47aa7",
        "decorator": {
            "foreground": "#85a7bf",
            "fontStyle": "italic"
        },
        "label": "#9aa1aa",
        "builtinType": "#d7c27e",
        "operator": "#dde1e6",
        "string.regexp": "#b9d3ea",
        "formatSpecifier": "#b9d3ea",
        "escape": "#b9d3ea",
        "number": "#dfc39a",
        "boolean": "#c4776f",
        "typeAlias": "#d7c27e"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8a9098"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#a47aa7"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#8fae6b"
            }
        },
        {
            "name": "Numbers & Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#dfc39a"
            }
        },
        {
            "name": "Regexp & Escape",
            "scope": [
                "string.regexp",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#c4776f"
            }
        },
        {
            "name": "Operators & Punctuation",
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.bracket",
                "meta.delimiter",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#dde1e6"
            }
        },
        {
            "name": "Functions & Methods",
            "scope": [
                "entity.name.function",
                "entity.name.method",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#85a7bf"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other.readwrite",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#dde1e6"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "meta.parameter"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "Classes, Types, Interfaces",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#d7c27e"
            }
        },
        {
            "name": "Interfaces",
            "scope": [
                "entity.name.type.interface",
                "support.type.interface"
            ],
            "settings": {
                "foreground": "#b8c8d8"
            }
        },
        {
            "name": "Enum & Enum Members",
            "scope": [
                "entity.name.type.enum",
                "variable.other.enummember",
                "constant.other.enummember"
            ],
            "settings": {
                "foreground": "#dfc39a"
            }
        },
        {
            "name": "Properties & Fields",
            "scope": [
                "variable.other.property",
                "variable.other.object.property",
                "support.variable.property",
                "meta.property.object",
                "meta.object-literal.key",
                "meta.member"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "Decorators / Attributes",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js",
                "meta.decorator",
                "meta.attribute",
                "entity.name.attribute",
                "entity.name.function.decorator",
                "variable.function.decorator"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#85a7bf"
            }
        },
        {
            "name": "Language Builtins",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.super",
                "variable.language.self",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c4776f"
            }
        },
        {
            "name": "[TS/JS] Functions",
            "scope": [
                "source.ts entity.name.function",
                "source.tsx entity.name.function",
                "source.js entity.name.function",
                "source.jsx entity.name.function",
                "source.ts entity.name.method",
                "source.tsx entity.name.method",
                "source.js entity.name.method",
                "source.jsx entity.name.method"
            ],
            "settings": {
                "foreground": "#85a7bf"
            }
        },
        {
            "name": "[TS/JS] Parameters",
            "scope": [
                "source.ts variable.parameter",
                "source.tsx variable.parameter",
                "source.js variable.parameter",
                "source.jsx variable.parameter"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "[TS/JS] Properties",
            "scope": [
                "source.ts variable.other.property",
                "source.ts meta.object-literal.key",
                "source.js variable.other.property",
                "source.js meta.object-literal.key",
                "support.variable.property.js",
                "support.variable.property.ts"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "[TS/JS] Classes & Types",
            "scope": [
                "source.ts entity.name.type",
                "source.ts entity.name.class",
                "source.tsx entity.name.type",
                "source.tsx entity.name.class",
                "source.ts entity.name.type.interface",
                "source.tsx entity.name.type.interface",
                "source.ts entity.name.type.enum",
                "source.tsx entity.name.type.enum"
            ],
            "settings": {
                "foreground": "#d7c27e"
            }
        },
        {
            "name": "[TS/JS] Interfaces",
            "scope": [
                "source.ts entity.name.type.interface",
                "source.tsx entity.name.type.interface"
            ],
            "settings": {
                "foreground": "#b8c8d8"
            }
        },
        {
            "name": "[TS/JS] Type Annotations",
            "scope": [
                "source.ts meta.type.annotation",
                "source.tsx meta.type.annotation",
                "source.ts storage.type",
                "source.tsx storage.type"
            ],
            "settings": {
                "foreground": "#d7c27e"
            }
        },
        {
            "name": "[JSX/TSX] Tags & Attributes",
            "scope": [
                "entity.name.tag.tsx",
                "entity.name.tag.jsx",
                "meta.tag.tsx",
                "meta.tag.jsx",
                "entity.other.attribute-name.tsx",
                "entity.other.attribute-name.jsx"
            ],
            "settings": {
                "foreground": "#c4776f"
            }
        },
        {
            "name": "[TS/JS] Decorators",
            "scope": [
                "source.ts meta.decorator",
                "source.tsx meta.decorator"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#85a7bf"
            }
        },
        {
            "name": "[Python] Functions",
            "scope": [
                "source.python entity.name.function",
                "source.python meta.function-call",
                "source.python support.function"
            ],
            "settings": {
                "foreground": "#85a7bf"
            }
        },
        {
            "name": "[Python] Parameters",
            "scope": [
                "source.python variable.parameter.function",
                "source.python variable.parameter"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "[Python] Classes & Types",
            "scope": [
                "source.python entity.name.class",
                "source.python support.type",
                "source.python support.class"
            ],
            "settings": {
                "foreground": "#d7c27e"
            }
        },
        {
            "name": "[Python] Builtins",
            "scope": [
                "source.python support.function.builtin",
                "source.python support.type.builtin",
                "constant.language.python"
            ],
            "settings": {
                "foreground": "#dfc39a"
            }
        },
        {
            "name": "[Python] self",
            "scope": [
                "source.python variable.language.self"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c4776f"
            }
        },
        {
            "name": "[C#] Methods",
            "scope": [
                "source.cs entity.name.method",
                "source.cs meta.method-call",
                "source.cs support.function"
            ],
            "settings": {
                "foreground": "#85a7bf"
            }
        },
        {
            "name": "[C#] Parameters",
            "scope": [
                "source.cs variable.parameter",
                "source.cs meta.parameter"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "[C#] Primary Constructor Parameters",
            "scope": [
                "source.cs variable.parameter.primary-constructor"
            ],
            "settings": {
                "foreground": "#e5c287"
            }
        },
        {
            "name": "[C#] Classes, Structs, Interfaces, Enums",
            "scope": [
                "source.cs entity.name.type",
                "source.cs entity.name.class",
                "source.cs entity.name.struct",
                "source.cs entity.name.interface",
                "source.cs entity.name.enum",
                "source.cs support.type",
                "source.cs support.class"
            ],
            "settings": {
                "foreground": "#d7c27e"
            }
        },
        {
            "name": "[C#] Namespace",
            "scope": [
                "source.cs entity.name.namespace",
                "source.cs support.namespace"
            ],
            "settings": {
                "foreground": "#8aa0b3"
            }
        },
        {
            "name": "[C#] Attributes (Annotations)",
            "scope": [
                "source.cs meta.attribute",
                "source.cs entity.name.attribute"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#85a7bf"
            }
        },
        {
            "name": "[C#] Keywords",
            "scope": [
                "source.cs keyword",
                "source.cs storage.type",
                "source.cs storage.modifier"
            ],
            "settings": {
                "foreground": "#a47aa7"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#d7c27e"
            }
        },
        {
            "name": "JSON Strings",
            "scope": [
                "source.json string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#b9d3ea"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": [
                "markup.heading",
                "markdown.heading",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#8fae6b"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.title.markdown",
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#7e8f9c"
            }
        }
    ]
}